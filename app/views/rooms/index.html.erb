<div class="row">
  <% @rooms.each do |room| %>
     <div class="col-md-3">
      <%= render 'shared/card',
          title: room.name,
          tag: room.tag,
          description: room.description,
          image: room.image_url,
          link: room_path(room.id) %>
    </div>
  <% end %>
</div>
